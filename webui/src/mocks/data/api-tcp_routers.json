[
  {
    "entryPoints": [
      "web-tcp"
    ],
    "service": "tcp-all",
    "rule": "HostSNI(`*`)",
    "status": "enabled",
    "middlewares": ["test-inflightconn", "test-ipwhitelist", "test-ipallowlist"],
    "name": "tcp-all@docker",
    "using": [
      "web-secured",
      "web"
    ],
    "priority": 10,
    "provider": "docker"
  },
  {
    "entryPoints": [
      "websecure-tcp"
    ],
    "service": "postgres",
    "rule": "HostSNI(`db.example.com`)",
    "tls": {
      "options": "default",
      "certResolver": "letsencrypt"
    },
    "status": "enabled",
    "name": "postgres@docker",
    "using": [
      "websecure-tcp"
    ],
    "priority": 20,
    "provider": "docker"
  }
]
